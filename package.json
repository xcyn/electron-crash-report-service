{
  "name": "electron-crash-report-service",
  "description": "Aggregate crash reports for Electron apps",
  "version": "1.0.6",
  "repository": "yoshuawuyts/electron-crash-report-service",
  "license": "MIT",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "start": "CRASH_REPORTS_PATH=/tmp/crash-reports NODE_ENV=development PORT=8080 nodemon . | pino-colada",
    "test": "standard && npm run deps",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "bin": {
    "electron-crash-report-service": "index.js"
  },
  "dependencies": {
    "envobj": "^1.0.3",
    "log-http": "^1.0.0",
    "mkdirp": "^0.5.1",
    "multer": "^1.3.0",
    "pino": "^4.2.4",
    "server-router": "^5.0.0"
  },
  "devDependencies": {
    "dependency-check": "^2.7.0",
    "nodemon": "^1.11.0",
    "pino-colada": "^1.3.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  }
}
